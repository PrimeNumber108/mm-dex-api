{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "dev": "nest start --watch --type-check",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest",
    "test:e2e": "vitest --config vitest.config-e2e.ts",
    "typeorm:cli": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm:cli -- -d src/libs/typeorm.config.ts migration:generate src/migrations/migrations",
    "migration:up": "npm run typeorm:cli migration:run -- -d src/libs/typeorm.config.ts",
    "migration:down": "npm run typeorm:cli -- -d src/libs/typeorm.config.ts migration:revert",
    "typechain:gen": "typechain --target ethers-v6 --out-dir src/contracts src/abis/**/*.json"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/cli": "^10.4.7",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^10.4.8",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/platform-socket.io": "^10.4.5",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.5",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/multer": "^1.4.12",
    "@types/supertest": "^6.0.2",
    "axios": "^1.7.7",
    "bignumber.js": "^9.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.2.0",
    "ethers": "^6.13.4",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "ioredis-mock": "^8.9.0",
    "joi": "^17.13.3",
    "json-bigint": "^1.0.0",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "minio": "^8.0.1",
    "morgan": "^1.10.0",
    "nestjs-minio-client": "^2.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "redis-mock": "^0.56.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.0",
    "swagger-ui-express": "^5.0.1",
    "typechain": "^8.3.2",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.2",
    "@swc/core": "^1.4.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.0.5"
  }
}
